{
  "title": "Study Notes -- 学习笔记",
  "author": "wangrusheng",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "GitHub": "https://github.com/wangrusheng/study"
    }
  },
  "plugins": [
    "-livereload",
    "image-captions",
    "github",
    "page-treeview",
    "editlink"
  ],
  "pluginsConfig": {
    "image-captions": {
      "attributes": {
        "width": "600"
      },
      "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    },
    "github": {
      "url": "https://github.com/wangrusheng/study"
    },
    "editlink": {
      "base": "https://github.com/wangrusheng/study/blob/master/",
      "label": "编辑本页"
    },
    "page-treeview": {
      "copyright": "Copyright &#169; wangrusheng",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    }
  }
}